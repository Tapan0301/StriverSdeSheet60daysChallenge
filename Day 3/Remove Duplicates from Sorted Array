int removeDuplicates(vector<int> &arr, int n) {
	// Write your code here.
    int nums=arr[0]-1;
    int count=0;
    for(int i=0;i<n;i++)
    {
        if(nums==arr[i])
        {
            continue;
        }
        else
        {
            nums=arr[i];
            count++;
        }
    }
    return count;
}
